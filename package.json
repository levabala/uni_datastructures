{
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "test_watch": "nodemon --ext ts --exec 'jasmine-ts --config=jasmine.json'",
    "coverage": "nyc -r lcov -r text -e .ts -x \"src/*.spec.ts\" npm run test_once",
    "build": "tsc",
    "setup": "yarn install && node_modules/jasmine/bin/jasmine.js init"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.3",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
